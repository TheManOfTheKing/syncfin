{
  "name": "conciliacao-bancaria-standalone",
  "version": "1.0.0",
  "description": "Sistema de Conciliação Bancária - Standalone White-Label",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "vercel-build": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "create-test-user": "tsx scripts/create-test-user.ts",
    "create-user-andre": "tsx scripts/create-user-andre.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.56.2",
    "@trpc/client": "^11.0.0-rc.502",
    "@trpc/react-query": "^11.0.0-rc.502",
    "@trpc/server": "^11.0.0-rc.502",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.11.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "superjson": "^2.2.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.5.5",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.24.2",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.5"
  }
}
